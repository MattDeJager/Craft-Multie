{
    "name": "matthew-de-jager/craft-multie",
    "description": "Simple craft multi-site management tools",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "matthewdejager5@gmail.com",
        "issues": "https://github.com/git@github.com:mattdejager/craft-multie.git/issues?state=open",
        "source": "https://github.com/git@github.com:mattdejager/craft-multie.git",
        "docs": "https://github.com/git@github.com:mattdejager/craft-multie.git",
        "rss": "https://github.com/git@github.com:mattdejager/craft-multie.git/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.6.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "matthewdejager\\craftmultie\\": "src/"
        }
    },
    "extra": {
        "handle": "multie",
        "name": "Multie",
        "developer": "Matthew De Jager",
        "documentationUrl": "https://github.com/git@github.com:mattdejager/craft-multie.git"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

